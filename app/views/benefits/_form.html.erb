
  <% if @benefit.errors.any? %>
  <div class="alert alert-danger" role="alert">
    <% @benefit.errors.full_messages.each do |message| %>
      <%= message %>
    <% end %>
  </div>
  <% end %>



<%= form_for(benefit) do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
      <%= f.submit class:'btn btn-primary' %>
  </p>
<% end %>
